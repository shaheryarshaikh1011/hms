<%- include('../partials/formpartial'); -%>





<div class="modal-dialog text-center form-page">
    <div class="col-sm-9 main-section">
    <h1>Add new doctor</h1>
      <div class="modal-content">
          <div class="col-12">
          <h1></h1>
        </div>

        <div class="col-12 form-input">
         <form action="addDoctor" method="POST">
            			<div class="form-group">
				
				<input class="form-control" type="text" placeholder="enter name" name="dname" required>
			</div>
			<div class="form-group">
				
				<input class="form-control" type="number" placeholder="enter Age" name="age" step="1" min=18 max=90 oninput="validity.valid||(value='');" required>
			</div>
			<div class="form-group">
				
				<input class="form-control" type="text" placeholder="enter Specialization" name="spec" required>
			</div>
		
		
            <button type="submit" class="btn btn-success">submit</button>
            <div class="goback">
            <a class="btn btn-warning btn-lg" href="/getDoctor">Go back!</a>
            </div>
          </form>


        </div>

   

      </div>
    </div>
</div>





<%- include('../partials/footer'); -%>